{
  "name": "myapp",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "$(npm bin)/concurrently --kill-others 'node ./bin/www' 'npm run watch'",
    "build": "browserify browser.js -o static/bundle.js",
    "watch": "$(npm bin)/watchify js/killme.js -p [ css-modulesify -o public/stylesheets/module.css ] -t reactify -o public/js/bundle.js --debug --verbose"
  },
  "dependencies": {
    "basex": "^0.7.0",
    "body-parser": "~1.15.1",
    "browserify": "^13.1.0",
    "client-ajax": "^0.1.6",
    "cookie-parser": "~1.4.3",
    "css-modulesify": "^0.25.1",
    "debug": "~2.2.0",
    "express": "~4.13.4",
    "jade": "~1.11.0",
    "morgan": "~1.7.0",
    "node-persist": "^2.0.2",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "serve-favicon": "~2.3.0"
  }
}
